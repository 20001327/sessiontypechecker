aspect AtomTyping{

    eq Atom.type(){
        if(getParent().getClass().getName().equals(Send.class.getName())){
            Send s = (Send) getParent();
            if(s.getMessage().getLabel().getIdent().equals("start_delegation")){
                return new AtS(s.getMessage().getSender().getIdent());
            }
        }
        return new AtomType();
    }
