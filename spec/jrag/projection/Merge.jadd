aspect Merge{

    public static List<Session> Session.merge(List<Session> sessions){
        List<Session> ret = new List();
        List<Session> recs = new List();

        for(int i = 0; i<sessions.getNumChild();i++){
            // TODO: devono essere tutte scelte esterne oppure End
            // TODO: se è presente un non connecting allora sono tutti ne connecting ne End
            // TODO: se è presente un connecting sono tutti o connecting oppure end
        }

        for(int i = 0; i<sessions.getNumChild();i++){
            if(sessions.getChild(i).isSubtypeOf(new ExternalChoice())){
                ExternalChoice ext = (ExternalChoice)sessions.getChild(i);
                ext.getReceivess().forEach(r->recs.add(r));
            }else if(sessions.getChild(i).isSubtypeOf(new SessionReceive())){
                recs.add((SessionReceive)sessions.getChild(i));
            }
        }

        for(int i = 0; i<recs.getNumChild();i++){
            boolean add = true;
            SessionReceive toBeAdded = recs.getChild(i);
            for(int j=0; j<ret.getNumChild();j++){
                SessionReceive present = ret.getChild(j);
                add = add && !present.equals(toBeAdded);
            }

            if(add){
                ret.add(toBeAdded);
            }
        }

        return ret;
    }

}